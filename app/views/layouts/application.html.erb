<!DOCTYPE html>
<html>

<head>
  <title>AlphaBlog</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application' %>
</head>

<body>
  <%= render 'layouts/messages' %>

  <%= render 'layouts/header' %>

  <div class="container">
    <%= yield %>
  </div>

  <%= render 'layouts/footer' %>

</body>
</html>
